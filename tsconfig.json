{
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": "./",
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react",
        "lib": [
            "dom.iterable",
            "DOM",
            "esnext",
            "scripthost",
        ],
        "module": "esnext",
        "moduleResolution": "bundler",
        "noImplicitAny": false,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "esnext",
        "types": [
            "unplugin-vue-router/client"
        ],
        "useDefineForClassFields": true,
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "./typed-router.d.ts",
        "src/**/*.d.ts",
        "src/**/*.js",
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.vue"
    ],
    "exclude": [
        "./dist",
        "./node_modules"
    ],
    "references": [
        {
            "path": "./tsconfig.node.json"
        }
    ]
}